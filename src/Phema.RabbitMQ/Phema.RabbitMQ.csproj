<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netcoreapp3.0</TargetFramework>
		<Version>3.1.4</Version>
		<RootNamespace>Phema.RabbitMQ</RootNamespace>
		<Authors>sergeyshaykhullin</Authors>
		<Description>
			.NET Core strongly typed RabbitMQ integration library
		</Description>
		<PackageTags>RabbitMQ;.NET Core;ASP.NET Core</PackageTags>
		<PackageProjectUrl>https://github.com/sergeyshaykhullin/Phema.RabbitMQ</PackageProjectUrl>
		<RepositoryType>git</RepositoryType>
